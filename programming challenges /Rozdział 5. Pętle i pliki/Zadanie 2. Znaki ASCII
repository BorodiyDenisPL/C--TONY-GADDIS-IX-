//Napisz program, który używa pętli do wyświetlana znaków dla kodów ASCII od 0 do 127 (34-127).
// Wyświetl 16 znaków w jednym wierszu.

#include <iostream>
using namespace std; 

int main(){

    for(int i = 33; i <= 127; ++i){
        cout.width(4);
        cout << static_cast<char>(i) << " ";
        cout.widen(4);
        cout << i;
        if (i % 16 == 0)
            cout << endl;
    }
    return 0;
}


